{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Debug(Test)",
            "type": "node",
            "request": "launch",
            "program": "${workspaceFolder}/node_modules/@rstest/core/bin/rstest.js",
            "args": ["run", "${file}"],
